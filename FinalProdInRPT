SELECT
    IFH.VBILLCODE               VBILLCODE
,   IFB.VCHANGERATE             VCHANGERATE
,   IFB.VBATCHCODE              VBATCHCODE
,   IFB.NSHOULDNUM              NSHOULDNUM
,   IFB.NSHOULDASSISTNUM        NSHOULDASSISTNUM
,   IFB.NNUM                    NNUM
,   IFB.NASSISTNUM              NASSISTNUM
,   IFB.VNOTEBODY               VNOTEBODY
,   FDW.NAME                    FDW
,   HUOWEI.NAME                 HUOWEI
,   BM.CODE                     MARTERCODE
,   BM.NAME                     MARTERNAME
,   BM.MATERIALTYPE             MATERIALTYPE
,   ZDW.NAME                    ZDW
,   TEAM.VTEAMNAME              BANZU
,   IFB.PK_GROUP                PK_GROUP
,   SUBSTR(IFB.DBIZDATE, 1, 4)
    YEAR
,   SUBSTR(IFB.DBIZDATE, 6, 2)
    MONTH
,   SUBSTR(IFB.DBIZDATE, 9, 2)  DAY
FROM
    IC_FINPRODIN_B  IFB
INNER JOIN
    IC_FINPRODIN_H  IFH
ON
    IFH.CGENERALHID =   IFB.CGENERALHID
INNER JOIN
    ORG_STOCKORG    OSO
ON
    IFB.PK_ORG  =   OSO.PK_STOCKORG
INNER JOIN
    BD_MEASDOC  FDW
ON
    IFB.CASTUNITID  =   FDW.PK_MEASDOC
INNER JOIN
    BD_RACK HUOWEI
ON
    IFB.CLOCATIONID =   HUOWEI.PK_RACK
INNER JOIN
    BD_MATERIAL BM
ON
    IFB.CMATERIALVID    =   BM.PK_MATERIAL
INNER JOIN
    BD_MEASDOC  ZDW
ON
    IFB.CUNITID =   ZDW.PK_MEASDOC
LEFT JOIN
    BD_TEAM TEAM
ON
    IFH.VDEF1   =   TEAM.CTEAMID
WHERE
    OSO.CODE    IN  (PARAMETER('osocode'))
